{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "talent-hub-host": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/talent-hub-host",
      "sourceRoot": "apps/talent-hub-host/src",
      "prefix": "app",
      "i18n": {
        "sourceLocale": "en",
        "locales": {
          "de": {
            "translation": "apps/talent-hub-host/src/assets/i18n/messages.de.json",
            "baseHref": "/de/"
          },
          "es": {
            "translation": "apps/talent-hub-host/src/assets/i18n/messages.es.json",
            "baseHref": "/es/"
          }
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-architects/native-federation:build",
          "options": {},
          "configurations": {
            "production": {
              "target": "talent-hub-host:esbuild:production"
            },
            "development": {
              "target": "talent-hub-host:esbuild:development",
              "dev": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-architects/native-federation:build",
          "options": {
            "target": "talent-hub-host:serve-original:development",
            "rebuildDelay": 500,
            "dev": true,
            "cacheExternalArtifacts": false,
            "port": 0
          }
        },
        "test": {
          "builder": "@angular/build:unit-test"
        },
        "esbuild": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "apps/talent-hub-host/src/main.ts",
            "tsConfig": "apps/talent-hub-host/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "apps/talent-hub-host/public"
              },
              {
                "glob": "manifest.webmanifest",
                "input": "apps/talent-hub-host/public",
                "output": "/"
              }
            ],
            "styles": ["apps/talent-hub-host/src/styles.scss"],
            "polyfills": ["es-module-shims", "@angular/localize/init"],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/talent-hub-ui/src/lib/styles"]
            },
            "localize": false
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/talent-hub-host/src/environments/environment.ts",
                  "with": "apps/talent-hub-host/src/environments/environment.prod.ts"
                }
              ],
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all",
              "serviceWorker": "apps/talent-hub-host/src/ngsw-config.json"
            },
            "de": {
              "localize": ["de"]
            },
            "es": {
              "localize": ["es"]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "assets": [
                {
                  "glob": "**/*",
                  "input": "apps/talent-hub-host/public"
                },
                {
                  "glob": "**/*",
                  "input": "apps/talent-hub-host/src/mocks",
                  "output": "/mocks"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-original": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "talent-hub-host:esbuild:production"
            },
            "development": {
              "buildTarget": "talent-hub-host:esbuild:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4200
          }
        }
      }
    },
